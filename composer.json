{
    "name": "igorkalm/idpcontroller",
    "description": "Some IDP Controller features integration with Laravel",
    "keywords": [
        "igorkalm",
        "idpcontroller"
    ],
    "homepage": "https://github.com/igorkalm/idpcontroller",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Igor Kalmykov",
            "email": "ikalm@viscomp.ru",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/routing": "^8.0",        
        "illuminate/http": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Igorkalm\\IDPcontroller\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Igorkalm\\IDPcontroller\\IDPcontrollerServiceProvider"
            ]
        }
    }
}
